 
      <div class='wrap'>
        <!--<div class="col-md-4 col-md-offset-4">-->
           

      <% @pin.each do |pin| %>

          <div class="post">
            <div class="panel panel-default">
              <div class="panel-heading">
                <h2><%= link_to pin.title, pin %></h2>
              </div>
              <div class="panel-body">
                <%= link_to (image_tag pin.image.url(:medium)), pin %>
              </div>
            </div>
          </div>
      <% end %>
    </div>
  

      